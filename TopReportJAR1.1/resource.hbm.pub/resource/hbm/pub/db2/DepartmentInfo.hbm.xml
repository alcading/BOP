<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.pub">
	<class
		name="DepartmentInfo"
		table="DEPARTMENT_INFO"
		lazy="false"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="departmentCode"
			type="long"
			column="DEPARTMENT_CODE"
		>
			<generator class="native">
				 
			</generator>
		</id>

		<property
			name="departmentName"
			column="DEPARTMENT_NAME"
			type="string"
			not-null="true"
			length="20"
		/>

		<property
			name="brcode"
			column="BRCODE"
			type="string"
			not-null="true"
			length="4"
		/>

		<property
			name="status"
			column="STATUS"
			type="string"
			not-null="true"
			length="1"
		/>

		<property
			name="lastUpdTlr"
			column="LAST_UPD_TLR"
			type="string"
			not-null="false"
			length="8"
		/>

		<property
			name="lastUpdDate"
			column="LAST_UPD_DATE"
			type="date"
			not-null="false"
			length="10"
		/>

		<property
			name="BZ"
			column="BZ"
			type="string"
			not-null="false"
			length="128"
		/>

		<property
			name="misc"
			column="MISC"
			type="string"
			not-null="false"
			length="128"
		/>

		<property
			name="misc2"
			column="MISC2"
			type="string"
			not-null="false"
			length="128"
		/>

		<property
			name="misc3"
			column="MISC3"
			type="string"
			not-null="false"
			length="128"
		/>

		<property
			name="misc4"
			column="MISC4"
			type="string"
			not-null="false"
			length="128"
		/>

	</class>
</hibernate-mapping>