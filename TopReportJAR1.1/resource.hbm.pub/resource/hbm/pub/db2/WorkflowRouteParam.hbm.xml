<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.ebank.entity.data.workflow">
	<class
		name="WorkflowRouteParam"
		table="WORKFLOW_ROUTE_PARAM"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="ID"
		>
			<generator class="native" >
            	 
            </generator>

		</id>

		<property
			name="stopId"
			column="STOP_ID"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="routeId"
			column="ROUTE_ID"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="brhClass"
			column="BRH_CLASS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="roleId"
			column="ROLE_ID"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="need"
			column="NEED"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="pass"
			column="PASS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="amtType"
			column="AMT_TYPE"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="approveAmt"
			column="APPROVE_AMT"
			type="big_decimal"
			not-null="false"
			length="15"
		/>


	</class>
</hibernate-mapping>