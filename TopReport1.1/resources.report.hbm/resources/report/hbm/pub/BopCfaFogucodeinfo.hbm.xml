<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BopCfaFogucodeinfo"
		table="BOP_CFA_FOGUCODEINFO"
	>
		<meta attribute="sync-DAO">false</meta>

		<id
			name="fogucodeinfoId"
			column="FOGUCODEINFO_ID"
			type="string"
			length="32"
		/>
		<property
			name="fogucode"
			column="FOGUCODE"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="foguname"
			column="FOGUNAME"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="fogunamen"
			column="FOGUNAMEN"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="fogurecode"
			column="FOGURECODE"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="guaranteetype"
			column="GUARANTEETYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="crtTm"
			column="CRT_TM"
			type="java.util.Date"
			not-null="false"
		/>
		<property
			name="recId"
			column="REC_ID"
			type="string"
			not-null="false"
			length="32"
		/>


	</class>	
</hibernate-mapping>