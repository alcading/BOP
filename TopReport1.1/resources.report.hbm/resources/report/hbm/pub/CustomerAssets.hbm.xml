<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="CustomerAssets"
		table="customer_assets"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="id" class="CustomerAssetsPK">
			<key-property
				name="updt"
				column="updt"
				type="java.util.Date"
			/>
			<key-property
				name="cnum"
				column="cnum"
				type="string"
			/>
		</composite-id>

		<property
			name="liveinamt"
			column="liveinamt"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="liveoutamt"
			column="liveoutamt"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="stillinamt"
			column="stillinamt"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="stilloutamt"
			column="stilloutamt"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="inamt"
			column="inamt"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="outamt"
			column="outamt"
			type="big_decimal"
			not-null="false"
			length="20"
		/>


	</class>	
</hibernate-mapping>