<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BiBusiNoConf"
		table="BI_BUSI_NO_CONF"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="id" class="BiBusiNoConfPK">
			<key-property
				name="apptype"
				column="APPTYPE"
				type="string"
			/>
			<key-property
				name="filetype"
				column="FILETYPE"
				type="string"
			/>
			<key-property
				name="busitype"
				column="BUSITYPE"
				type="string"
			/>
		</composite-id>
		<property
			name="confpath"
			column="CONFPATH"
			type="string"
			not-null="false"
			length="256"
		/>


	</class>	
</hibernate-mapping>