<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="TlrManRel"
		table="TLR_MAN_REL"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="manage"
			column="Manage"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="tlrId"
			column="TLR_ID"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="timestamps"
			column="timestamps"
			type="date"
			not-null="false"
			length="23"
		/>
		<property
			name="misc1"
			column="misc1"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="misc2"
			column="misc2"
			type="string"
			not-null="false"
			length="256"
		/>


	</class>	
</hibernate-mapping>