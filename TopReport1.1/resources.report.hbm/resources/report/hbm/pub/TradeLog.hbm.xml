<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="TradeLog"
		table="trade_log"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="id" class="TradeLogPK">
			<key-property
				name="txdate"
				column="txdate"
				type="date"
			/>
			<key-property
				name="id"
				column="id"
				type="string"
			/>
		</composite-id>

		<property
			name="cnum"
			column="cnum"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="cmrm"
			column="cmrm"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="ccy"
			column="ccy"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="pstd"
			column="pstd"
			type="date"
			not-null="false"
			length="23"
		/>
		<property
			name="vald"
			column="vald"
			type="date"
			not-null="false"
			length="23"
		/>
		<property
			name="trat"
			column="trat"
			type="string"
			not-null="false"
			length="5"
		/>
		<property
			name="pnar"
			column="pnar"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="psta"
			column="psta"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="drcr"
			column="drcr"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="solId"
			column="sol_id"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="tranAmtCny"
			column="tran_amt_cny"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="cnyRate"
			column="cny_rate"
			type="big_decimal"
			not-null="false"
			length="9"
		/>
		<property
			name="ccyFlag"
			column="ccy_flag"
			type="string"
			not-null="false"
			length="5"
		/>
		<property
			name="tranType"
			column="tran_type"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="miscflg"
			column="miscflg"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="desc1"
			column="desc1"
			type="string"
			not-null="false"
			length="254"
		/>
		<property
			name="desc2"
			column="desc2"
			type="string"
			not-null="false"
			length="254"
		/>


	</class>	
</hibernate-mapping>