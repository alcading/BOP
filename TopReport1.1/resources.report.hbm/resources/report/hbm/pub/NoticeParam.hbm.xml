<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="NoticeParam"
		table="notice_param"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
		<generator class="native">
				 
			</generator>
		</id>

		<property
			name="brcode"
			column="brcode"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="createdt"
			column="createdt"
			type="date"
			not-null="false"
			length="23"
		/>
		<property
			name="noticeName"
			column="notice_name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="noticeValue"
			column="notice_value"
			type="big_decimal"
			not-null="false"
			length="18"
		/>
		<property
			name="noticeAmt"
			column="notice_amt"
			type="big_decimal"
			not-null="false"
			length="18"
		/>
		<property
			name="noticeTotperi"
			column="notice_totperi"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="noticeFlg"
			column="notice_flg"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="updt"
			column="updt"
			type="date"
			not-null="false"
			length="23"
		/>
		<property
			name="miscOne"
			column="misc_one"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="miscTwo"
			column="misc_two"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="miscflgs"
			column="miscflgs"
			type="string"
			not-null="false"
			length="1"
		/>


	</class>	
</hibernate-mapping>