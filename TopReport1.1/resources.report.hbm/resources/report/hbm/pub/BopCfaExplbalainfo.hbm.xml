<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BopCfaExplbalainfo"
		table="BOP_CFA_EXPLBALAINFO"
	>
		<meta attribute="sync-DAO">false</meta>

		<id
			name="explbalainfoId"
			column="EXPLBALAINFO_ID"
			type="string"
			length="32"
		/>
		<property
			name="explcurr"
			column="EXPLCURR"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="explamount"
			column="EXPLAMOUNT"
			type="big_decimal"
			not-null="false"
			length="22"
		/>
		<property
			name="explbala"
			column="EXPLBALA"
			type="big_decimal"
			not-null="false"
			length="22"
		/>
		<property
			name="explperamount"
			column="EXPLPERAMOUNT"
			type="big_decimal"
			not-null="false"
			length="22"
		/>
		<property
			name="plcoseamount"
			column="PLCOSEAMOUNT"
			type="big_decimal"
			not-null="false"
			length="22"
		/>
		<property
			name="crtTm"
			column="CRT_TM"
			type="java.util.Date"
			not-null="false"
		/>
		<property
			name="recId"
			column="REC_ID"
			type="string"
			not-null="false"
			length="32"
		/>


	</class>	
</hibernate-mapping>