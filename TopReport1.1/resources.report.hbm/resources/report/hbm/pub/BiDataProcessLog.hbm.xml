<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BiDataProcessLog"
		table="BI_DATA_PROCESS_LOG"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="PROC_ID"
		>
		</id>

		<property
			name="apptype"
			column="APPTYPE"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="currentfile"
			column="CURRENTFILE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="recId"
			column="REC_ID"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="busiNo"
			column="BUSI_NO"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="execType"
			column="EXEC_TYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="execResult"
			column="EXEC_RESULT"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="execRemark"
			column="EXEC_REMARK"
			type="string"
			not-null="false"
			length="1024"
		/>
		<property
			name="execTm"
			column="EXEC_TM"
			type="java.util.Date"
			not-null="false"
			length="11"
		/>
		<property
			name="execTlr"
			column="EXEC_TLR"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="filler1"
			column="FILLER1"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="filler2"
			column="FILLER2"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="filler3"
			column="FILLER3"
			type="string"
			not-null="false"
			length="64"
		/>


	</class>	
</hibernate-mapping>