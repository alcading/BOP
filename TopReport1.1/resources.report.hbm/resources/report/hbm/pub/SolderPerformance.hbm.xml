<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="SolderPerformance"
		table="solder_performance"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="id" class="SolderPerformancePK">
			<key-property
				name="cmrm"
				column="cmrm"
				type="string"
			/>
			<key-property
				name="moths"
				column="moths"
				type="string"
			/>
			<key-property
				name="dtyp"
				column="dtyp"
				type="string"
			/>
		</composite-id>

		<property
			name="solId"
			column="sol_id"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="amt"
			column="amt"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="st"
			column="ST"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="lock"
			column="IS_LOCK"
			type="true_false"
			not-null="false"
			length="1"
		/>
		<property
			name="del"
			column="IS_DEL"
			type="true_false"
			not-null="false"
			length="1"
		/>
		<property
			name="updt"
			column="updt"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="lastOperator"
			column="last_operator"
			type="string"
			not-null="false"
			length="12"
		/>


	</class>	
</hibernate-mapping>