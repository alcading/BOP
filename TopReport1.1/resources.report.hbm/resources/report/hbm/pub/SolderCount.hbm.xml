<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="SolderCount"
		table="solder_count"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="id" class="SolderCountPK">
			<key-property
				name="updt"
				column="updt"
				type="timestamp"
			/>
			<key-property
				name="solId"
				column="sol_id"
				type="string"
			/>
			<key-property
				name="cmrm"
				column="cmrm"
				type="string"
			/>
		</composite-id>

		<property
			name="incust"
			column="incust"
			type="big_decimal"
			not-null="false"
			length="12"
		/>
		<property
			name="outcust"
			column="outcust"
			type="big_decimal"
			not-null="false"
			length="12"
		/>
		<property
			name="inamt"
			column="inamt"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="outamt"
			column="outamt"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="onlyamt"
			column="onlyamt"
			type="big_decimal"
			not-null="false"
			length="20"
		/>


	</class>	
</hibernate-mapping>