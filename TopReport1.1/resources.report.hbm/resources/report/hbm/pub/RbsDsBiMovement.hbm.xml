<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="RbsDsBiMovement"
		table="RBS_DS_BI_MOVEMENT"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
		</id>

		<property
			name="changedate"
			column="CHANGEDATE"
			type="string"
			not-null="true"
			length="8"
		/>
		<property
			name="changetime"
			column="CHANGETIME"
			type="string"
			not-null="true"
			length="8"
		/>
		<property
			name="changetype"
			column="CHANGETYPE"
			type="string"
			length="30"
		/>
		<property
			name="changeobject"
			column="CHANGEOBJECT"
			type="string"
			length="30"
		/>
		<property
			name="changefield"
			column="CHANGEFIELD"
			type="string"
			length="50"
		/>
		<property
			name="databeforechange"
			column="DATABEFORECHANGE"
			type="string"
			length="50"
		/>
		<property
			name="dataafterchange"
			column="DATAAFTERCHANGE"
			type="string"
			length="50"
		/>
		<property
			name="entriedby"
			column="ENTRIEDBY"
			type="string"
			length="128"
		/>
		<property
			name="authorizedby"
			column="AUTHORIZEDBY"
			type="string"
			length="128"
		/>
	</class>
</hibernate-mapping>