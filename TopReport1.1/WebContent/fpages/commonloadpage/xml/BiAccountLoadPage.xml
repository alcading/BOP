<?xml version="1.0" encoding="UTF-8"?>
<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	navigate:导航信息
	type:方式（sql|call)
	interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号
	transdataactionurl:数据整理URL
	txnlogflag:true记录日志
	async:异步提交标志
	pagesize:页面显示记录数(缺省为10)
-->

<CommQry title="BiAccountEntry.title" navigate="基础信息管理  &gt; 客户帐户信息维护"
    type="call" interface = "false" pagesize="10" async="true" databusid=""
    transdataactionurl="/trans/TransDataAction.do" txnlogflag="true"
    getterclassname="com.huateng.report.loadpage.getter.BiAccountLoadPageGetter" >
   <Include id="BankParam" />
<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->

		<Field id="id" desc="BiAccountEntry.field.id" require="true" edittype="text"  size="64"  datatype="string" status="F" xpath="/id"  />
		<Field id="accountCur" desc="BiAccountEntry.field.accountCur" require="true" edittype="select" size="3"  datatype="string" status="F" 
			translated="CQ:CurrencyLoadPageSelect" dropdowntype="dynamic" method="None" viewField="currencyName" 
			fieldmap="accountCur=currencyNo;accountCurName=currencyName" xpath="/accountCur" />

		<Field id="accountCurName" desc="BiAccountEntry.field.accountCur" status="F" 
			method="com.huateng.ebank.business.common.CQMethod.getCurNameByCurNo"
			width="" readonly="" primary="true" type="String" size="150"
			xpath="/accountCur" default=""/>
			
		<Field id="customerId" desc="BiAccountEntry.field.customerId" require="true" size="32"   edittype="text" datatype="string" status="F" xpath="/customerId" />
		<Field id="accountType" desc="BiAccountEntry.field.accountType" require="true" edittype="select" status="A" translated="DATA_DIC.2004" datatype="string" xpath="/accountType" />

		<Field id="accountProperty" desc="BiAccountEntry.field.accountProperty" edittype="text" datatype="string" type="String" xpath="/accountProperty" />
		<Field id="openTime" desc="BiAccountEntry.field.openTime" edittype="date" datatype="date" primary="false" type="String" size="20" xpath="/openTime" />
		<Field id="closeTime" desc="BiAccountEntry.field.closeTime" edittype="date" datatype="date" primary="false" type="String" size="20" xpath="/closeTime" />

		<Field id="abroadFlag" desc="BiAccountEntry.field.abroadFlag" edittype="text" datatype="int" xpath="/abroadFlag" />
		<Field id="enCode" desc="BiAccountEntry.field.enCode" edittype="text" datatype="string" xpath="/enCode" />
		<Field id="chineseName" desc="BiAccountEntry.field.chineseName" edittype="text" datatype="string" xpath="/chineseName" />
		<Field id="fileNumber" desc="BiAccountEntry.field.fileNumber"  require="true" edittype="text" datatype="string" xpath="/fileNumber" />
		<Field id="isnraFlag" desc="BiAccountEntry.field.isNRAFlag" edittype="checkbox" datatype="int" size="1" xpath="/isnraFlag" />

		<Field id="customerIdExt" desc="BiAccountEntry.field.customerIdExt" edittype="text" datatype="string" size="1" xpath="/customerIdExt" />

		<Field id="debits" desc="BiAccountEntry.field.debits"   edittype="text" datatype="currency" scale="2" xpath="/debits" />
		<Field id="credits" desc="BiAccountEntry.field.credits" edittype="text" datatype="currency" scale="2" xpath="/credits" />
		<Field id="balance" desc="BiAccountEntry.field.balance" edittype="text" datatype="currency" scale="2" xpath="/balance" />
		<Field id="dealDate" desc="BiAccountEntry.field.dealDate" edittype="date" datatype="string" xpath="/dealDate" />

		<Field id="filler1" desc="BiAccountEntry.field.filler1" edittype="text" datatype="string" xpath="" />
		<Field id="filler2" desc="BiAccountEntry.field.filler2" edittype="text" datatype="string" xpath="" />
		<Field id="filler3" desc="BiAccountEntry.field.filler3" edittype="text" datatype="string" xpath="" />

</Fields>

<Where >
	<TextBox id="qAccountId" desc="BiAccountEntry.field.id" datatype="string" index="0" />
	<Option id="qAccountCur" operationb="EQ" size="" index="1"
			desc="BiAccountEntry.field.accountCur" tip="BiAccountEntry.field.accountCur" type="string" translated="CQ:CurrencyLoadPageSelect"
			fieldmap="qaccountCur=currencyNo;qaccountCurName=currencyNoName" viewField="currencyNoName" dropdowntype="dynamic" init="true" require="false" readOnly=""/>
	<TextBox id="qCustomerId" desc="BiAccountEntry.field.customerId" datatype="string" index="2"/>
	<Option id="qAccountType" dataType="String" edittype="select" size="10" desc="BiAccountEntry.field.accountType"
			tip="BiAccountEntry.field.accountType" translated="DATA_DIC.2004" operation="EQ" init="true"
			method="None" index="3" require="" readOnly="" />
</Where>

<Operations>
	<Button id="btSure" name="btSure" desc ="确定" operation="" url="#" show="" group="" txn="" />
	<Button id="btSureBack" name="btSureBack" desc="取消" operation="" url="#" show="" group="" txn="" />
	
</Operations>
	<Script></Script>
 </CommQry>