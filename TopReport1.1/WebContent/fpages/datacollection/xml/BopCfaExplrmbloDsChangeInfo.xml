<CommQry title="外汇质押人民币贷款变动信息" navigate="上报数据补录 &gt; 外汇质押人民币贷款变动信息补录 &gt; 变动信息"
	type="call"
	interface="true"
	pagesize="10"
	async="true"
	databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	getterclassname="com.huateng.report.getter.BopCfaExplrmbloDsChangeInfoGetter">
	<Include id="BankParam" />
	<Fields>
		<!-- status :F-基本信息中显示 N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传  -->
		<Field id="id" desc="记录主键" require="" edittype="text"
			datatype="string" method="None" readonly="false" size="32" status="F"
			default="" primary="true" xpath="/id" />

		<Field id="actiontype" desc="操作类型" require="" edittype="select"
			datatype="string" method="None" readonly="false" size="1" status="F"
			default="" xpath="/actiontype" translated="DATA_DIC.10" />

		<Field id="actiondesc" desc="删除原因" require="" edittype="text"
			datatype="string" method="None" readonly="false" size="128"
			status="F" default="" xpath="/actiondesc" />

		<Field id="explrmblono" desc="外汇质押人民币贷款编号" require="true"
			edittype="text" datatype="string" method="None" readonly="false"
			size="28" status="F" default="" xpath="/explrmblono" />

		<Field id="filler2" desc="业务流水号" require="true" edittype="text" datatype="string" method="None"
	 	  readonly="false" size="32" status="F" default="" xpath="/filler2" />

		<!-- 已经 被报告期取代
		<Field id="buscode" desc="银行业务参号" require="true" edittype="text"
			datatype="string" method="None" readonly="false" size="32" status="F"
			default="" xpath="/buscode" />
		 -->

		<!-- "银行业务参号" 数据项改成 "报告期" -->
		<Field id="buocmonth" desc="报告期" status="F" primary="false"
			method="None" readonly="false" type="String" size="20" default=""
			edittype="select" fieldmap="buocmonth=yyyyMM;buocmonthName=yyyyMM"
			viewField="yyyyMM" dropdowntype="custom" tip="报告期"
			url="/templets/lib/yyyyMM.ftl" init="false" xpath="/buocmonth"
			require="true" />
		<Field id="buocmonthName" desc="报告期" status="F"
			width="" readonly="" primary="true" type="String" size="150"
			xpath="/buocmonth" default="" />


		<Field id="changeno" desc="变动编号" require="true" edittype="text"
			datatype="string" method="None" readonly="false" size="4" status="F"
			default="" xpath="/changeno" />


		<Field id="monbeloadbal" desc="月初贷款余额" require="true" edittype="text"
			datatype="currency" method="None" readonly="false" size="22" scale="2" status="F"
			default="" xpath="/monbeloadbal" />

		<Field id="monenloadbal" desc="月末贷款余额" require="true" edittype="text"
			datatype="currency" method="None" readonly="false" size="22" scale="2" status="F"
			default="" xpath="/monenloadbal" />


		<Field id="credwithamount" desc="本月提款金额" require="true" edittype="text"
			datatype="currency" method="None" readonly="false" size="22" scale="2" status="F"
			default="" xpath="/credwithamount" />

		<Field id="credrepayamount" desc="本月还本金额" require="true" edittype="text"
			datatype="currency" method="None" readonly="false" size="22" scale="2" status="F"
			default="" xpath="/credrepayamount" />

		<Field id="picamount" desc="本月付息费金额" require="true" edittype="text"
			datatype="currency" method="None" readonly="false" size="22" scale="2" status="F"
			default="" xpath="/picamount" />



		<Field id="remark" desc="备注" require="" edittype="textarea"
			datatype="string" method="None" readonly="false" size="256"
			status="F" default="" xpath="/remark" />

		<Field id="lstUpdTm" desc="最后更新时间" require="" edittype="date"
			datatype="timestamp" method="None" readonly="false" size="32" status="F"
			default="" xpath="/lstUpdTm" />

		<Field id="crtTm" desc="创建时间" require="" edittype="date"
			datatype="timestamp" method="None" readonly="false" size="32" status="F"
			default="" xpath="/crtTm" />


		<!-- 状态 -->

		<Field id="apptype" desc="应用类型" require="" edittype="select"
			datatype="string" method="None" readonly="false" size="3" status="F"
			default="" xpath="/apptype" translated="DATA_DIC.17" />

		<Field id="currentfile" desc="文件类型" require="" edittype="select"
			datatype="string" method="None" readonly="false" size="2"
			translated="DATA_DIC.19" status="F" default="" xpath="/currentfile" />

		<Field id="lstUpdTlr" desc="最后更新人" require="" edittype="text"
			datatype="string" method="None" readonly="false" size="20" status="F"
			default="" xpath="/lstUpdTlr" />

		<Field id="brNo" desc="机构号" require="" edittype="text"
			datatype="text" method="None" readonly="false" size="20" status="F"
			default="" xpath="/brNo" />

		<Field id="recStatus" desc="记录状态" require="" edittype="select"
			datatype="string" method="None" readonly="false" size="2" status="F"
			default="" xpath="/recStatus"
			translated="DATA_DIC.62" />

		<Field id="repStatus" desc="回执状态" require="" edittype="select"
			datatype="string" method="None" readonly="false" size="2" status="F"
			default="" xpath="/repStatus"
			translated="DATA_DIC.63" />

		<Field id="approveStatus" desc="审核状态" require=""
			edittype="select" datatype="string" method="None" readonly="false"
			size="2" status="F" default="" xpath="/approveStatus"
			translated="DATA_DIC.64" />

		<Field id="approveResult" desc="审核说明" require="" edittype="text"
			datatype="string" method="None" readonly="false" size="256"
			status="F" default="" xpath="/approveResult" />

		<Field id="workDate" desc="工作日期" require="" edittype="date"
			datatype="date" method="None" readonly="false" size="8" status="F"
			default="" xpath="/workDate" />

		<Field id="subSuccess" desc="是否已成功上报" require="" edittype="text"
			datatype="string" method="None" readonly="false" size="1" status="F"
			default="" xpath="/subSuccess" />

		<Field id="opr" desc="操作" status="N" method="None" xpath=""
			primary="true" type="" size="20" default="" tip="操作" require="" />

	</Fields>

	<Where>

		<DateTextBox id="qworkDateStart" desc="工作日期开始" tip="工作日期开始" require="false"
			status="F" datatype="date" method="None" size="150" width=""
			primary="false" index="0" type="String" />
			
		<DateTextBox id="qworkDateEnd" desc="工作日期结束" tip="工作日期结束" require="false"
			status="F" datatype="date" method="None" size="150" width=""
			primary="false" index="1" type="String" />	

		<Option id="actiontype" desc="操作类型" edittype="select"
			datatype="string" size="1" index="2" translated="DATA_DIC.10" />

		<Option id="recStatus" desc="记录状态" edittype="select"
			datatype="string" size="2" index="3"
			translated="LIST:01,可编辑;02,编辑待确认" />

		<Option id="approveStatus" desc="审核状态" edittype="select"
			datatype="string" size="2" index="4"
			translated="DATA_DIC.64" />

		<Option id="repStatus" desc="回执状态" edittype="select"
			datatype="string" size="2" index="5"
			translated="DATA_DIC.63" />

		<TextBox id="filler2" desc="业务流水号" edittype="text"
			status="F" datatype="string" size="28" index="6" />

	</Where>

	<Operations>
		<Button id="btSave" name="btSave" desc="保存" operation="" url="#" show="" group="" txn="" />
		<Button id="btMod" name="btMod" desc="补录" operation="" url="#" show="" group="" txn="" />
		<Button id="btDelete" name="btDelete" desc="删除" operation="" url="#" show="" group="" txn="" />
		<Button id="btDetail" name="btDetail" desc="详细" operation="" url="#" show="" group="" txn="" />
		<Button id="btAdd" name="btAdd" desc="新增" operation="" url="#" show="" group="" txn="" icon="icon-add"/>
	</Operations>
	<Sql>
	</Sql>
</CommQry>