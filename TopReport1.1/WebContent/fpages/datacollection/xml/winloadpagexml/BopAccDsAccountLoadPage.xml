<?xml version="1.0" encoding="UTF-8"?>
<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	navigate:导航信息
	type:方式（sql|call)
	interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号
	transdataactionurl:数据整理URL
	async:异步提交标志
	pagesize:页面显示记录数(缺省为10)
-->
<CommQry title="账户开关信息" navigate="上报数据补录&gt; 外汇账户信息补录 &gt; 账户开关信息"
	type="call"
	interface="true"
	pagesize="10"
	async="true"
	databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	getterclassname="com.huateng.report.getter.BopAccDsAccountLoadPageGetter">
	<Include id="BankParam" />
	<Fields>
		<!-- status :F-基本信息中显示 N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->
		<Field id="id" desc="记录主键" require="" edittype="text" datatype="string" method="None"
		  readonly="false" size="32" status="F" default="" primary="true" xpath="/id" />
	 	<Field id="accountno" desc="账号" require="false" edittype="text" datatype="string" method="None"
	 	  readonly="false" size="64" status="F" default="" xpath="/accountno" />

	 	<Field id="accountstat" desc="账户状态" require="false" edittype="select" datatype="string" method="None"
	 		readonly="false" size="2" translated="DATA_DIC.11" status="F" default="" xpath="/accountstat" />

	 	<Field id="amtype" desc="开户主体类型" require="false" edittype="select" datatype="string" method="None"
	 		readonly="false" size="2" translated="DATA_DIC.12" status="F" default="" xpath="/amtype" />

		<Field id="enCode" desc="开户主体代码" require="false" edittype="text" datatype="string" method="None"
	 	  readonly="false" size="18" status="F" default="" xpath="/enCode" />

		<Field id="enName" desc="开户主体名称" require="false" edittype="text" datatype="string" method="None"
	 	  readonly="false" size="128" status="F" default="" xpath="/enName" />

	 	<Field id="accountType" desc="账户性质代码" require="false" edittype="select" datatype="string" method="None"
	 		readonly="false" size="4" translated="DATA_DIC.13" status="F" default="" xpath="/accountType" />

		<Field id="accountCata" desc="账户类别" require="false" edittype="select" datatype="string" method="None"
			readonly="false" size="2" translated="DATA_DIC.14" status="F" default="" xpath="/accountCata" />

	 	<Field id="currencyCode" desc="币种" status="F" primary="false"
			method="None" readonly="false" type="String" size="20" default=""
			edittype="select" translated="CQ:CurrencySelect"
			fieldmap="currencyCode=currencyNo;currencyCodeName=currencyNoName"
			viewField="currencyNoName" xpath="/currencyCode" require="false"
			tip="币种"/>

		<Field id="currencyCodeName" desc="币种" status="F" require=""
			method="com.huateng.ebank.business.common.CQMethod.getCurNameByCurNo"
			width="" readonly="" primary="true" type="String" size="150"
			xpath="/currencyCode" default=""/>

		<Field id="businessDate" desc="业务发生日期" require="false" edittype="date" datatype="date" method="None"
	 	  readonly="false" size="32" status="F" default="" xpath="/businessDate" />

	 	<Field id="fileNumber" desc="外汇局批件号/备案表号/业务编号" require="" edittype="text" datatype="string" method="None"
	 	  readonly="false" size="28" status="F" default="" xpath="/fileNumber" />

		<Field id="limitType" desc="限额类型" require="false" edittype="select" datatype="string" method="None"
		  readonly="false" size="10" translated="DATA_DIC.15" status="F" default="" xpath="/limitType" />

		<Field id="accountLimit" desc="账户限额" require="" edittype="text" datatype="currency" scale="2" method="None"
	 	  readonly="false" size="32" status="F" default="" xpath="/accountLimit" />

		<Field id="workDate" desc="工作日期" require="" edittype="date" datatype="date" method="None"
	 	  readonly="false" size="32" status="F" default="" xpath="/workDate" />

	</Fields>

	<Where>
		<DateTextBox id="qStartDate" desc="开始工作日期" tip="开始工作日期"
			require="false" status="F" datatype="date" method="None" size="150" width=""
			primary="false" index="0" type="String" />

		<DateTextBox id="qEndDate" desc="结束工作日期" tip="结束工作日期"
			require="false" status="F" datatype="date" method="None" size="150" width=""
			primary="false" index="1" type="String" />

		<Option id="qaccountCata" dataType="String" edittype="select" size="10" desc="账户类别"
			tip="账户类别" translated="DATA_DIC.14" operation="EQ" init="true"
			method="None" index="2" require="" readOnly="" />

		<Option id="qaccountstat" dataType="String" edittype="select" size="10" desc="账户状态"
			tip="账户状态" translated="DATA_DIC.11" operation="EQ" init="true"
			method="None" index="3" require="" readOnly="" />

		<TextBox id="qaccountno" desc="帐号" datatype="string" index="4" />
	</Where>

	<Operations>
		<Button id="btConfirm" name="btConfirm" desc="确定" operation=""
			url="" show="" group="" txn="" />
		<Button id="btConfirmBack" name="btConfirmBack" desc="返回" operation=""
			url="" show="" group="" txn="" />
	</Operations>

	<Sql>
	</Sql>
</CommQry>